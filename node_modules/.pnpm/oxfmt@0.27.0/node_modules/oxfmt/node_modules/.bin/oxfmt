#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/oxfmt@0.27.0/node_modules/oxfmt/bin/node_modules:/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/oxfmt@0.27.0/node_modules/oxfmt/node_modules:/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/oxfmt@0.27.0/node_modules:/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/oxfmt@0.27.0/node_modules/oxfmt/bin/node_modules:/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/oxfmt@0.27.0/node_modules/oxfmt/node_modules:/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/oxfmt@0.27.0/node_modules:/home/dennis/Desktop/code/npm/scaffold-tanstack-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/oxfmt" "$@"
else
  exec node  "$basedir/../../bin/oxfmt" "$@"
fi
